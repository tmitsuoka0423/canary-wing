{
  "name": "canary-wing",
  "version": "2.0.0",
  "description": "",
  "main": "./dist/index.js",
  "scripts": {
    "start": "yarn webpack && NODE_ENV=DEV electron ./dist/index.js",
    "tsc": "tsc",
    "watch": "tsc -w",
    "package:mac": "yarn webpack && electron-builder --mac --x64",
    "package:windows": "yarn webpack && electron-builder --win --x64",
    "package:linux": "yarn webpack && electron-builder --linux --x64",
    "webpack": "webpack",
    "prewebpack": "yarn"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tmitsuoka0423/dev-launcher.git"
  },
  "keywords": [],
  "author": "Takahiro Mitsuoka <tmitsuoka0423@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tmitsuoka0423/dev-launcher/issues"
  },
  "homepage": "https://github.com/tmitsuoka0423/dev-launcher#readme",
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^12.12.6",
    "@typescript-eslint/eslint-plugin": "^3.5.0",
    "@typescript-eslint/parser": "^3.5.0",
    "copy-webpack-plugin": "^6.0.3",
    "electron": "^9.4.0",
    "electron-builder": "^22.7.0",
    "eslint": "^7.3.1",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.2",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "prettier": "^2.0.5",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.5",
    "webpack": "^5.1.0",
    "webpack-cli": "^3.3.12"
  },
  "build": {
    "productName": "CanaryWing",
    "appId": "canary-wing.tmitsuoka0423.com",
    "directories": {
      "output": "./product"
    },
    "files": [
      "./dist/**/*",
      "./icon/icon.icns"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "./icon/icon.icns",
      "target": [
        "dmg"
      ]
    },
    "win": {
      "target": "msi"
    }
  }
}
